{
  "name": "ts-test",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k -p \"[{name}]\" -n \"WebpackDevServe,Typescript,Server\" -c \"yellow.bold,blue.bold,green.bold\" \"webpack-dev-server\" \"tsc -b -w ./src/desktop ./src/server\" \"yarn serve\"",
    "serve": "nodemon -w dist/server/ dist/server/server.js",
    "desktop": "concurrently -k -p \"[{name}]\" -n \"Server,Desktop\" -c \"yellow.bold,blue.bold\" \"yarn serve\" \"yarn run-desktop\"",
    "run-desktop": "nodemon -w dist/desktop/ --exec yarn electron dist/desktop/main.js",
    "test": "NODE_ENV=test jest --watchAll"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.1.0",
    "@types/cookie-session": "^2.0.37",
    "@types/express": "^4.17.2",
    "@types/express-fileupload": "^1.1.0",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "@types/string-similarity": "^3.0.0",
    "@types/styled-components": "^4.4.2",
    "@types/supertest": "^2.0.8",
    "@types/webpack": "^4.39.8",
    "@types/webpack-dev-middleware": "^2.0.3",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.3.3",
    "electron": "^7.1.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.6",
    "immutability-helper": "^3.0.1",
    "jest": "^24.9.0",
    "libxmljs2": "^0.22.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-dropzone": "^10.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "string-similarity": "^3.0.0",
    "styled-components": "^5.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.4",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0"
  }
}
