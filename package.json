{
  "name": "ts-test",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k -p \"[{name}]\" -n \"WebpackDevServe,Typescript\" -c \"yellow.bold,blue.bold\" \"webpack-dev-server\" \"tsc -b -w src/desktop src/server\"",
      
      "serve": "nodemon -w dist/server/ dist/server/server.js",
      "desktop" :"concurrently -k -p \"[{name}]\" -n \"Server,Desktop\" -c \"yellow.bold,blue.bold\" \"yarn serve\" \"yarn run-desktop\"",
      "run-desktop": "nodemon -w dist/desktop/ --exec yarn electron dist/desktop/main.js"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.39.8",
    "@types/webpack-dev-middleware": "^2.0.3",
    "axios": "^0.19.0",
    "electron": "^7.1.0",
    "express": "^4.17.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.4",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0"
  }
}
